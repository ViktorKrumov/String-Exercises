using System;
using System.Text;

namespace Stringosvane
{
    class Program
    {
        static void Main(string[] args)
        {
            string input = Console.ReadLine();

            if (input.Length < 20)
            {
                int result = 20 - input.Length;
                Console.Write(input + new String('*', result));
            }
            else
            {
                Console.WriteLine(input);
            }

        }
    }
}
